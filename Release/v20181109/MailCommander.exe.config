    <add name="oracle" connectionString="Data Source=TNS_NAME;User ID=USER_NAME;Password=PASS_WORD" providerName="Oracle.DataAccess.Client" />
  </connectionStrings>
  <appSettings>
    <add key="AllowedMails" value="" />
    <add key="RestrictedMails" value="" />
